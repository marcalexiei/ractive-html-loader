{
  "name": "ractive-html-loader",
  "version": "1.0.0",
  "author": "Marco Pasqualetti @marcalexiei",
  "description": "Ractive HTML loader module for Webpack",
  "license": "MIT",
  "main": "dist/loader.js",
  "keywords": [
    "webpack",
    "ractive",
    "html",
    "loader"
  ],
  "engines": {
    "node": ">= 16.13.1",
    "npm": ">= 8.1.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky || true",
    "clean": "del-cli dist",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production babel src --out-dir dist --copy-files"
  },
  "dependencies": {
    "fastparse": "^1.1.2",
    "loader-utils": "^3.2.0",
    "schema-utils": "^4.0.0"
  },
  "peerDependencies": {
    "ractive": "1.x",
    "webpack": "5.x"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "babel-jest": "29.7.0",
    "del-cli": "5.1.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.29.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "memfs": "4.6.0",
    "ractive": "1.4.2",
    "webpack": "5.90.1"
  },
  "homepage": "https://github.com/marcalexiei/ractive-html-loader",
  "repository": "https://github.com/marcalexiei/ractive-html-loader.git",
  "bugs": "https://github.com/marcalexiei/ractive-html-loader/issues"
}
