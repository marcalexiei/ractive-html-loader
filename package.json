{
  "name": "ractive-html-loader",
  "version": "2.0.0",
  "author": "Marco Pasqualetti @marcalexiei",
  "description": "Ractive HTML loader module for Webpack",
  "license": "MIT",
  "type": "module",
  "main": "dist/loader.js",
  "exports": "./dist/loader.js",
  "keywords": [
    "webpack",
    "ractive",
    "html",
    "loader"
  ],
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky || true",
    "clean": "del-cli dist",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production babel src --out-dir dist --copy-files"
  },
  "dependencies": {
    "fastparse": "^1.1.2",
    "loader-utils": "^3.2.0",
    "schema-utils": "^4.0.0"
  },
  "peerDependencies": {
    "ractive": "1.x",
    "webpack": "5.x"
  },
  "devDependencies": {
    "@babel/cli": "7.25.6",
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.4",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "babel-jest": "29.7.0",
    "del-cli": "5.1.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.30.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "memfs": "4.12.0",
    "prettier": "^3.3.3",
    "ractive": "1.4.4",
    "webpack": "5.94.0"
  },
  "homepage": "https://github.com/marcalexiei/ractive-html-loader",
  "repository": "https://github.com/marcalexiei/ractive-html-loader.git",
  "bugs": "https://github.com/marcalexiei/ractive-html-loader/issues"
}
